  <body>
    <%- include('../views/partials/header.ejs') %>
    <img
      class="background__raining"
      src="./static/assets/raining-background.jpg"
      alt="raining in the background"
    />
    <main>
      <div class="col-md-8 mb-2">
      <a href="/biljetter" class="btn btn-primary hem__button">Hem</a>
      </div>
      <div class="container mt-5">      
      <h1>Skapa konto</h1>     
          
        <div class="col-md-6 offset-md-3">
            
          <form id="signup__form">
            <div class="form__group">
              <label for="fullname" class="form__label">Fullständigt namn:</label>
              <input type="text" class="form__control custom__height" id="fullname" placeholder="Fullständigt namn" required />
            </div>
        
            <div class="form__group">
              <label for="email" class="form__label">E-post:</label>
              <input type="email" class="form__control custom__height" id="email" placeholder="E-post" required />
            </div>
        
            <div class="form__group">
              <label for="username" class="form__label">Användarnamn:</label>
              <input type="text" class="form__control custom__height username" placeholder="Användarnamn" required autocomplete="username" />
            </div>
        
            <div class="form__group">
              <label for="password" class="form__label">Lösenord:</label>
              <input type="password" class="form__control custom__height password" placeholder="Lösenord" required autocomplete="new-password" />
            </div>
        
            <button type="submit" class="btn btn-success skapa__button">Skapa konto</button>
          </form>
        </div>
      </div>
        </main>
        
      
    <%- include('../views/partials/footer.ejs') %>
    <script src="/static/signup.js"></script>
  </body>
</html>

